        password = 123456
        shell_client = ssh
        shell_port = 22
        file_transfer_client = scp
        file_transfer_port = 22
        mem_chk_cmd = dmidecode -t 17 | awk -F: '/Size/ {print $2}'
        mem_chk_cur_cmd = grep MemTotal /proc/meminfo
        cpu_chk_cmd = grep -c processor /proc/cpuinfo
        unattended_install:
